		call BlzFrameSetPoint(CombinedCameraSliderSystem___sliderRotationLabel, CombinedCameraSliderSystem___framepointVerticalMain, CombinedCameraSliderSystem___sliderRotation, CombinedCameraSliderSystem___framepointVerticalSecondary, labelGap, 0)
		call BlzFrameSetMinMaxValue(CombinedCameraSliderSystem___sliderRotation, udg_CCSS_RotationMin, udg_CCSS_RotationMax)
		call BlzFrameSetValue(CombinedCameraSliderSystem___sliderRotation, udg_CCSS_RotationDefault)
