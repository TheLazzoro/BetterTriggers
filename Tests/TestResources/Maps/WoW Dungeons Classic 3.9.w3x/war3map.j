function Trig_AR_Loop_Actions takes nothing returns nothing
    set udg_AR_Loop=1
    loop
        exitwhen udg_AR_Loop > udg_AR_MUI
        if ( Trig_AR_Loop_Func001Func001C() ) then
            // The If, Then, Else Action below checks if the range the unit has been moved is less than the Effects end.
            if ( Trig_AR_Loop_Func001Func001Func002C() ) then
                // Calculating some distances for the Effect.
                set udg_AR_Speed[udg_AR_Loop]=20.00
                set udg_AR_DisCount[udg_AR_Loop]=( udg_AR_DisCount[udg_AR_Loop] + udg_AR_Speed[udg_AR_Loop] )
                // Setting up point variables for the Move Effect & Special Effects.
                set udg_AR_Loc[1]=GetUnitLoc(udg_AR_Cstr[udg_AR_Loop])
                set udg_AR_Loc[2]=PolarProjectionBJ(udg_AR_Loc[1], udg_AR_Speed[udg_AR_Loop], AngleBetweenPoints(udg_AR_Loc[1], udg_AR_TPOABC[udg_AR_Loop]))
                // Moving the unit.
                call SetUnitPositionLoc(udg_AR_Cstr[udg_AR_Loop], udg_AR_Loc[2])
                // Again, adding points to variables for special effects,
                set udg_AR_Loc[4]=PolarProjectionBJ(udg_AR_Loc[1], 50.00, ( GetUnitFacing(udg_AR_Cstr[udg_AR_Loop]) - 90.00 ))
                set udg_AR_Loc[5]=PolarProjectionBJ(udg_AR_Loc[1], 50.00, ( GetUnitFacing(udg_AR_Cstr[udg_AR_Loop]) - 270.00 ))
                // Creating the Special Effects
                set udg_AR_SFX_Loop1=4
                loop
                    exitwhen udg_AR_SFX_Loop1 > 5
                    call AddSpecialEffectLocBJ(udg_AR_Loc[udg_AR_SFX_Loop1], "Abilities\\Spells\\Undead\\ReplenishMana\\SpiritTouchTarget.mdl")
                    call DestroyEffectBJ(GetLastCreatedEffectBJ())
                    call RemoveLocation(udg_AR_Loc[udg_AR_SFX_Loop1])
                    set udg_AR_SFX_Loop1=udg_AR_SFX_Loop1 + 1
                endloop
                // Storing all the units thats going to be damaged into a variable and damages them and adding some nice Special Effects
                set udg_AR_DmgGroup=GetUnitsInRangeOfLocMatching(250.00, udg_AR_Loc[2], Condition(function Trig_AR_Loop_Func001Func001Func002Func029002003))
                call ForGroupBJ(udg_AR_DmgGroup, function Trig_AR_Loop_Func001Func001Func002Func030A)
                call EnumDestructablesInCircleBJ(250.00, udg_AR_Loc[2], function Trig_AR_Loop_Func001Func001Func002Func031A)
                // Clearing Leaks
                call DestroyGroup(udg_AR_DmgGroup)
                call RemoveLocation(udg_AR_Loc[1])
                call RemoveLocation(udg_AR_Loc[2])
            else
                // This is the end of the effect. Setting some variables to NULL and changing unit back to normal
                set udg_AR_DisCount[udg_AR_Loop]=0.00
                call RemoveLocation(udg_AR_TPOABC[udg_AR_Loop])
                call DestroyEffectBJ(udg_AR_SFX1[udg_AR_Loop])
                call GroupClear(udg_AR_SafeGroup[udg_AR_Loop])
                call SetUnitTimeScalePercent(udg_AR_Cstr[udg_AR_Loop], 100.00)
                call PauseUnitBJ(false, udg_AR_Cstr[udg_AR_Loop])
                call SetUnitPathing(udg_AR_Cstr[udg_AR_Loop], true)
                // This is for the Multi-Unit Instanceability.
                // AWARE! DO NOT CHANGE!
                set udg_AR_Cstr[udg_AR_Loop]=null
                set udg_AR_Index=( udg_AR_Index - 1 )
                set udg_AR_Boolean[udg_AR_Loop]=false
                if ( Trig_AR_Loop_Func001Func001Func002Func015C() ) then
                    call DisableTrigger(gg_trg_AR_AnimLoop)
                    call DisableTrigger(GetTriggeringTrigger())
                    set udg_AR_MUI=0
                else
                endif
            endif
        else
        endif
        set udg_AR_Loop=udg_AR_Loop + 1
    endloop
endfunction

