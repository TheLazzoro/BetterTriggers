function Trig_Raz_spell_Conditions takes nothing returns boolean
    if ( not ( IsUnitAliveBJ(gg_unit_n02N_0520) == true ) ) then
        return false
    endif
    if ( not ( GetUnitStateSwap(UNIT_STATE_LIFE, gg_unit_n02N_0520) < ( I2R(GetUnitLevel(gg_unit_n02N_0520)) * ( I2R(udg_Mode) * 180.00 ) ) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Raz_spell_Func007C takes nothing returns boolean
    if ( not ( CountUnitsInGroup(udg_TempBossGroup) == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Raz_spell_Actions takes nothing returns nothing
    set udg_TempBossGroup=GetUnitsInRangeOfLocAll(900.00, GetUnitLoc(gg_unit_n02N_0520))
    call GroupRemoveGroup(GetUnitsOfPlayerAll(Player(PLAYER_NEUTRAL_AGGRESSIVE)), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsOfTypeIdAll('h00K'), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsInRangeOfLocAll(200.00, GetUnitLoc(gg_unit_n02N_0520)), udg_TempBossGroup)
    if ( Trig_Raz_spell_Func007C() ) then
        call DestroyGroup(udg_TempBossGroup)
        call DestroyEffectBJ(udg_TempBossEffect)
        return
    else
        set udg_TempUnit=GroupPickRandomUnit(udg_TempBossGroup)
        set udg_TempBossLoc=GetUnitLoc(udg_TempUnit)
        call UnitDamagePointLoc(gg_unit_n02N_0520, 1.45, 200.00, udg_TempBossLoc, ( 200.00 * udg_ModeDmgReal ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
        call AddSpecialEffectLocBJ(udg_TempBossLoc, "war3mapImported\\Frosty Crystal Drop.mdx")
        set udg_TempBossEffect=GetLastCreatedEffectBJ()
        call PlaySoundAtPointBJ(gg_snd_FrostNovaTarget1, 100, udg_TempBossLoc, 0)
        call RemoveLocation(udg_TempBossLoc)
        call DestroyGroup(udg_TempBossGroup)
        call TriggerSleepAction(1.50)
        call DestroyEffectBJ(udg_TempBossEffect)
    endif
endfunction

