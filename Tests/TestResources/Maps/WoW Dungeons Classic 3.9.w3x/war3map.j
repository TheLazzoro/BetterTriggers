function Trig_Zumbrah_Shadow_Bolt_Actions takes nothing returns nothing
    set udg_TempBossGroup=GetUnitsInRangeOfLocAll(900.00, GetUnitLoc(gg_unit_n01E_0149))
    call GroupRemoveGroup(GetUnitsOfPlayerAll(Player(PLAYER_NEUTRAL_AGGRESSIVE)), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsOfTypeIdAll('h00K'), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsInRangeOfLocAll(200.00, GetUnitLoc(gg_unit_n01E_0149)), udg_TempBossGroup)
    if ( Trig_Zumbrah_Shadow_Bolt_Func007C() ) then
        call DestroyGroup(udg_TempBossGroup)
        call DestroyEffectBJ(udg_TempBossEffect)
        return
    else
        set udg_TempUnit=GroupPickRandomUnit(udg_TempBossGroup)
        set udg_TempBossLoc=GetUnitLoc(udg_TempUnit)
        call UnitDamagePointLoc(gg_unit_n01E_0149, 1.00, 100.00, udg_TempBossLoc, ( 250.00 * udg_ModeDmgReal ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
        call AddSpecialEffectLocBJ(udg_TempBossLoc, "war3mapImported\\Voidfall Medium.mdx")
        set udg_TempBossEffect=GetLastCreatedEffectBJ()
        call RemoveLocation(udg_TempBossLoc)
        call DestroyGroup(udg_TempBossGroup)
        call TriggerSleepAction(1.20)
        call DestroyEffectBJ(udg_TempBossEffect)
    endif
