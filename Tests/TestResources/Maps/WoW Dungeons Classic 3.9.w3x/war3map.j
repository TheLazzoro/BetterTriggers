function Trig_Fireball_Actions takes nothing returns nothing
    // BonusMultipliersAdd
    set udg_TempRealBonus=2.55
    // BonusMultipliersEnd
    set udg_TempReal=( I2R(GetHeroStatBJ(bj_HEROSTAT_INT, GetTriggerUnit(), true)) * udg_TempRealBonus )
    if ( Trig_Fireball_Func005C() ) then
        set udg_TempReal=( udg_TempReal * 1.11 )
    else
    endif
    if ( Trig_Fireball_Func006C() ) then
        set udg_TempReal=( udg_TempReal * 1.09 )
    else
    endif
    if ( Trig_Fireball_Func007C() ) then
        set udg_TempReal=( udg_TempReal * 1.17 )
    else
    endif
    if ( Trig_Fireball_Func008C() ) then
        set udg_TempReal=( udg_TempReal * 1.15 )
    else
    endif
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_IndexEnchant
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Fireball_Func009Func001C() ) then
            set udg_TempReal=( udg_TempReal * 1.10 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set udg_TempReal=( udg_TempReal * udg_FireballReal[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] )
    if ( Trig_Fireball_Func011C() ) then
        set udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + 10 )
        set udg_TempInt=GetRandomInt(1, 100)
        if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
            set udg_TempReal=( udg_TempReal * 2.00 )
        else
            call DoNothing()
        endif
        if ( Trig_Fireball_Func011Func008C() ) then
            if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
                call BlzEndUnitAbilityCooldown(GetTriggerUnit(), 'A06L')
            else
                call DoNothing()
            endif
            if ( Trig_Fireball_Func011Func008Func002C() ) then
                call AddSpecialEffectTargetUnitBJ("origin", GetTriggerUnit(), "Abilities\\Spells\\Items\\TomeOfRetraining\\TomeOfRetrainingCaster.mdl")
                call DestroyEffectBJ(GetLastCreatedEffectBJ())
            else
            endif
        else
        endif
        set udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] - 10 )
    else
        set udg_TempInt=GetRandomInt(1, 100)
        if ( Trig_Fireball_Func011Func003C() ) then
            if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
                call BlzEndUnitAbilityCooldown(GetTriggerUnit(), 'A06L')
            else
                call DoNothing()
            endif
            if ( Trig_Fireball_Func011Func003Func002C() ) then
                call AddSpecialEffectTargetUnitBJ("origin", GetTriggerUnit(), "Abilities\\Spells\\Items\\TomeOfRetraining\\TomeOfRetrainingCaster.mdl")
                call DestroyEffectBJ(GetLastCreatedEffectBJ())
            else
            endif
        else
        endif
        if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
            set udg_TempReal=( udg_TempReal * 2.00 )
        else
            call DoNothing()
        endif
    endif
    set udg_TempInt=R2I(udg_TempReal)
    call UnitDamageTargetBJ(GetTriggerUnit(), GetSpellTargetUnit(), I2R(udg_TempInt), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
endfunction

