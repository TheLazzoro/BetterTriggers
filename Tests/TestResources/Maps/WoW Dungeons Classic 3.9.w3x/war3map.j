    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[1]), function Trig_PvPStartMove_Func020A)
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[2]), function Trig_PvPStartMove_Func022A)
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[3]), function Trig_PvPStartMove_Func024A)
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[4]), function Trig_PvPStartMove_Func026A)
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[5]), function Trig_PvPStartMove_Func028A)
    set bj_wantDestroyGroup=true
    call ForGroupBJ(GetUnitsOfPlayerAll(udg_PvPPlayer[6]), function Trig_PvPStartMove_Func030A)
    if ( Trig_PvPStartMove_Func031C() ) then
        set udg_TempLoc=GetRectCenter(gg_rct_PvPTeamAlliance)
        call SetUnitPositionLoc(udg_Unit[1], udg_TempLoc)
