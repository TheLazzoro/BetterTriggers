    else
        set udg_TempReal=( I2R(GetHeroStatBJ(bj_HEROSTAT_INT, GetTriggerUnit(), true)) * 3.57 )
    endif
    if ( Trig_Flamestrike_Func002C() ) then
        set udg_TempReal=( udg_TempReal * 1.11 )
    else
