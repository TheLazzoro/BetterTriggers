function Trig_Display_Damage_Actions takes nothing returns nothing
    set udg_Integer[45]=R2I(GetEventDamage())
    set udg_Integer[46]=( udg_Integer[45] / 6 )
    set udg_Point[45]=GetUnitLoc(GetAttackedUnitBJ())
    set udg_Point[46]=GetUnitLoc(GetEventDamageSource())
    if ( Trig_Display_Damage_Func005C() ) then
        set udg_Point[47]=PolarProjectionBJ(udg_Point[45], ( 5.00 + I2R(udg_Integer[46]) ), AngleBetweenPoints(udg_Point[46], udg_Point[45]))
    else
        set udg_Point[47]=PolarProjectionBJ(udg_Point[45], 90.00, AngleBetweenPoints(udg_Point[46], udg_Point[45]))
    endif
    if ( Trig_Display_Damage_Func006C() ) then
        call PauseUnitBJ(true, GetAttackedUnitBJ())
        call SetUnitPositionLoc(GetAttackedUnitBJ(), udg_Point[47])
        call PauseUnitBJ(false, GetAttackedUnitBJ())
        if ( Trig_Display_Damage_Func006Func006C() ) then
            call IssueTargetOrderBJ(GetAttackedUnitBJ(), "attack", GetEventDamageSource())
        else
        endif
    else
    endif
    if ( Trig_Display_Damage_Func007C() ) then
        set udg_StringSize=( IAbsBJ(udg_Integer[45]) / 100 )
        call CreateTextTagLocBJ(( "" + I2S(IAbsBJ(udg_Integer[45])) ), udg_Point[45], 50.00, ( 9.00 + I2R(udg_StringSize) ), 100.00, 100.00, 10.00, 0.00)
        if ( Trig_Display_Damage_Func007Func005C() ) then
            set udg_DthDmgInt[1]=( udg_DthDmgInt[1] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[1]=I2S(udg_DthDmgInt[1])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 2, udg_MultiDamage[1])
        else
        endif
        if ( Trig_Display_Damage_Func007Func006C() ) then
            set udg_DthDmgInt[2]=( udg_DthDmgInt[2] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[2]=I2S(udg_DthDmgInt[2])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 3, udg_MultiDamage[2])
        else
        endif
        if ( Trig_Display_Damage_Func007Func007C() ) then
            set udg_DthDmgInt[3]=( udg_DthDmgInt[3] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[3]=I2S(udg_DthDmgInt[3])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 4, udg_MultiDamage[3])
        else
        endif
        if ( Trig_Display_Damage_Func007Func008C() ) then
            set udg_DthDmgInt[4]=( udg_DthDmgInt[4] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[4]=I2S(udg_DthDmgInt[4])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 5, udg_MultiDamage[4])
        else
        endif
        if ( Trig_Display_Damage_Func007Func009C() ) then
            set udg_DthDmgInt[5]=( udg_DthDmgInt[5] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[5]=I2S(udg_DthDmgInt[5])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 6, udg_MultiDamage[5])
        else
        endif
        if ( Trig_Display_Damage_Func007Func010C() ) then
            set udg_DthDmgInt[6]=( udg_DthDmgInt[6] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiDamage[6]=I2S(udg_DthDmgInt[6])
            call MultiboardSetItemValueBJ(udg_Multiboard, 4, 7, udg_MultiDamage[6])
        else
        endif
        if ( Trig_Display_Damage_Func007Func011C() ) then
            set udg_DthTankInt[GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ()))]=( udg_DthTankInt[GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ()))] + IAbsBJ(udg_Integer[45]) )
            set udg_MultiTaken[GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ()))]=I2S(udg_DthTankInt[GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ()))])
            call MultiboardSetItemValueBJ(udg_Multiboard, 6, ( GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ())) + 1 ), udg_MultiTaken[GetConvertedPlayerId(GetOwningPlayer(GetAttackedUnitBJ()))])
        else
        endif
    else
        if ( Trig_Display_Damage_Func007Func002C() ) then
            call CreateTextTagLocBJ(( "+" + I2S(IAbsBJ(udg_Integer[45])) ), udg_Point[45], 50.00, 10, 0.00, 100.00, 0.00, 0)
        else
        endif
    endif
    call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
    call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 100.00, AngleBetweenPoints(udg_Point[46], udg_Point[45]))
    call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 1.50)
    call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
    call RemoveLocation(udg_Point[45])
    call RemoveLocation(udg_Point[46])
    call RemoveLocation(udg_Point[47])
endfunction

