		else
			call BlzFrameSetAbsPoint(CombinedCameraSliderSystem___sliderHeight, CombinedCameraSliderSystem___framepointVerticalMain, udg_CCSS_PositionSlidersX, udg_CCSS_PositionSlidersY)
