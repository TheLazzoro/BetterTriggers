        return false
    endif
    return true
endfunction

function Trig_Flash_Heal_Func008001 takes nothing returns boolean
    return ( udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] )
endfunction

function Trig_Flash_Heal_Func023C takes nothing returns boolean
    if ( not ( udg_TalentInspirationBol[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Flash_Heal_Actions takes nothing returns nothing
    set udg_TempReal=( I2R(GetHeroStatBJ(bj_HEROSTAT_INT, GetTriggerUnit(), true)) * 4.75 )
    if ( Trig_Flash_Heal_Func002C() ) then
        set udg_TempReal=( udg_TempReal * 1.11 )
    else
    endif
    if ( Trig_Flash_Heal_Func003C() ) then
        set udg_TempReal=( udg_TempReal * 1.18 )
    else
    endif
    if ( Trig_Flash_Heal_Func004C() ) then
        set udg_TempReal=( udg_TempReal * 1.16 )
    else
    endif
    if ( Trig_Flash_Heal_Func005C() ) then
        set udg_TempReal=( udg_TempReal * 1.18 )
    else
    endif
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_IndexEnchant
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Flash_Heal_Func006Func001C() ) then
            set udg_TempReal=( udg_TempReal * 1.10 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set udg_TempInt=GetRandomInt(1, 100)
    if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
        set udg_TempReal=( udg_TempReal * 2.00 )
    else
        call DoNothing()
    endif
    set udg_TempInt=R2I(udg_TempReal)
    // HealingStart
    set udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + R2I(udg_TempReal) )
    call MultiboardSetItemValueBJ(udg_Multiboard, 5, ( GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit())) + 1 ), I2S(udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]))
    set udg_TempLoc=GetUnitLoc(GetSpellTargetUnit())
    set udg_StringSize=( R2I(udg_TempReal) / 100 )
    call CreateTextTagLocBJ(I2S(R2I(udg_TempReal)), udg_TempLoc, 60.00, ( 9.00 + I2R(udg_StringSize) ), 0.00, 100.00, 0.00, 0)
    call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
    call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 70.00, 90)
    call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 2.00)
    call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
    call RemoveLocation(udg_TempLoc)
    // HealingStop
    call SetUnitLifeBJ(GetSpellTargetUnit(), ( GetUnitStateSwap(UNIT_STATE_LIFE, GetSpellTargetUnit()) + udg_TempReal ))
    if ( Trig_Flash_Heal_Func023C() ) then
        call AddSpecialEffectTargetUnitBJ("overhead", GetSpellTargetUnit(), "Abilities\\Spells\\Items\\AIma\\AImaTarget.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        set udg_TempTalentInspirationReal[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( I2R(GetHeroStatBJ(bj_HEROSTAT_STR, GetTriggerUnit(), true)) * 2.00 )
        // ManaStart
        set udg_TempLoc=GetUnitLoc(GetSpellTargetUnit())
        set udg_StringSize=( R2I(udg_TempReal) / 100 )
        call CreateTextTagLocBJ(I2S(R2I(udg_TempReal)), udg_TempLoc, 55.00, ( 9.00 + I2R(udg_StringSize) ), 0.00, 0.00, 100.00, 0)
        call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
        call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 70.00, 90)
        call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 2.00)
        call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
        call RemoveLocation(udg_TempLoc)
        // ManaStop
        call SetUnitManaBJ(GetSpellTargetUnit(), ( GetUnitStateSwap(UNIT_STATE_MANA, GetSpellTargetUnit()) + udg_TempTalentInspirationReal[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] ))
    else
    endif
endfunction

