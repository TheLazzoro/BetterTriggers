        return false
    endif
    return true
endfunction

function Trig_AR_Start_Func003C takes nothing returns boolean
    if ( not ( udg_AR_Index == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_AR_Start_Actions takes nothing returns nothing
    // Setting up some important variables. Also, preparing the unit for the Effect.
    // Here i fix the whole spell MUI (Multi-Unit Instanceability)
    if ( Trig_AR_Start_Func003C() ) then
        call EnableTrigger(gg_trg_AR_Loop)
        call EnableTrigger(gg_trg_AR_AnimLoop)
    else
    endif
    set udg_AR_Index=( udg_AR_Index + 1 )
    set udg_AR_MUI=( udg_AR_MUI + 1 )
    set udg_AR_Boolean[udg_AR_MUI]=true
    // Adding the Caster, Casters Location, The targeted point where the spell been casted and the Distance between the caster and the Targeted Point to VARIABLES.
    set udg_AR_Cstr[udg_AR_MUI]=GetTriggerUnit()
    set udg_AR_CstrPos[udg_AR_MUI]=GetUnitLoc(udg_AR_Cstr[udg_AR_MUI])
    set udg_AR_TPOABC[udg_AR_MUI]=GetSpellTargetLoc()
    set udg_AR_DisCalc1[udg_AR_MUI]=DistanceBetweenPoints(udg_AR_CstrPos[udg_AR_MUI], udg_AR_TPOABC[udg_AR_MUI])
    // Creating some nice SFX on the unit.
    call AddSpecialEffectTargetUnitBJ("hand, left", udg_AR_Cstr[udg_AR_MUI], "Abilities\\Spells\\Undead\\DevourMagic\\DevourMagicBirthMissile.mdl")
    set udg_AR_SFX1[udg_AR_MUI]=GetLastCreatedEffectBJ()
    // Clearing a Leak...
    call RemoveLocation(udg_AR_CstrPos[udg_AR_MUI])
    // Turning the Collision off and Pausing the unit for easier work.
    call SetUnitPathing(udg_AR_Cstr[udg_AR_MUI], false)
    call PauseUnitBJ(true, GetTriggerUnit())
    // Making the Animation 300% faster than normal.
    call SetUnitTimeScalePercent(udg_AR_Cstr[udg_AR_MUI], 250.00)
endfunction

