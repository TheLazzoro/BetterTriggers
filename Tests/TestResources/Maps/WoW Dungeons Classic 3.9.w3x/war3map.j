    if ( Trig_AddTimeSeconds_Func003C() ) then
        if ( Trig_AddTimeSeconds_Func003Func001C() ) then
            if ( Trig_AddTimeSeconds_Func003Func001Func001C() ) then
                if ( Trig_AddTimeSeconds_Func003Func001Func001Func001C() ) then
                    set udg_MultiTimeInteger[2]=0
                    set udg_MultiTimeInteger[4]=0
                    set udg_MultiTimeInteger[1]=0
                    set udg_MultiTimeInteger[5]=0
                    set udg_MultiTimeInteger[3]=( udg_MultiTimeInteger[3] + 1 )
                    set udg_MultiMin10=I2S(udg_MultiTimeInteger[5])
                    set udg_MultiSec10=I2S(udg_MultiTimeInteger[4])
                    set udg_MultiMin=( ( ":" + udg_MultiMin10 ) + I2S(udg_MultiTimeInteger[2]) )
                    set udg_MultiSec=( ( ":" + udg_MultiSec10 ) + I2S(udg_MultiTimeInteger[1]) )
                    set udg_MultiHour=I2S(udg_MultiTimeInteger[3])
                else
                    set udg_MultiTimeInteger[2]=0
                    set udg_MultiTimeInteger[4]=0
                    set udg_MultiTimeInteger[1]=0
                    set udg_MultiTimeInteger[5]=( udg_MultiTimeInteger[5] + 1 )
                    set udg_MultiMin10=I2S(udg_MultiTimeInteger[5])
                    set udg_MultiSec10=I2S(udg_MultiTimeInteger[4])
                    set udg_MultiMin=( ( ":" + udg_MultiMin10 ) + I2S(udg_MultiTimeInteger[2]) )
                    set udg_MultiSec=( ( ":" + udg_MultiSec10 ) + I2S(udg_MultiTimeInteger[1]) )
                endif
            else
                set udg_MultiTimeInteger[2]=( udg_MultiTimeInteger[2] + 1 )
                set udg_MultiTimeInteger[4]=0
                set udg_MultiMin=( ( ":" + udg_MultiMin10 ) + I2S(udg_MultiTimeInteger[2]) )
                set udg_MultiSec10=I2S(udg_MultiTimeInteger[4])
                set udg_MultiTimeInteger[1]=0
                set udg_MultiSec=( ( ":" + udg_MultiSec10 ) + I2S(udg_MultiTimeInteger[1]) )
            endif
        else
            set udg_MultiTimeInteger[4]=( udg_MultiTimeInteger[4] + 1 )
            set udg_MultiSec10=I2S(udg_MultiTimeInteger[4])
            set udg_MultiTimeInteger[1]=0
            set udg_MultiSec=( ( ":" + udg_MultiSec10 ) + I2S(udg_MultiTimeInteger[1]) )
        endif
    else
    endif
    // Show
    set udg_MultiTime=( udg_MultiHour + ( udg_MultiMin + udg_MultiSec ) )
    call MultiboardSetItemValueBJ(udg_Multiboard, 4, 9, udg_MultiTime)
