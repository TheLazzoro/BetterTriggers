function Trig_Holy_Smite_Heal_Conditions takes nothing returns boolean
    if ( not ( GetOwningPlayer(GetTriggerUnit()) != Player(PLAYER_NEUTRAL_AGGRESSIVE) ) ) then
        return false
    endif
    if ( not ( UnitHasBuffBJ(GetSpellTargetUnit(), 'B01V') == true ) ) then
        return false
    endif
    return true
endfunction

function Trig_Holy_Smite_Heal_Actions takes nothing returns nothing
    // HealingStart
    set udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + R2I(udg_TempReal) )
    call MultiboardSetItemValueBJ(udg_Multiboard, 5, ( GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit())) + 1 ), I2S(udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]))
    set udg_TempLoc=GetUnitLoc(GetTriggerUnit())
    call CreateTextTagLocBJ(I2S(R2I(( GetUnitStateSwap(UNIT_STATE_MAX_LIFE, GetTriggerUnit()) / 10.00 ))), udg_TempLoc, 60.00, 10.00, 0.00, 100.00, 0.00, 0)
    call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
    call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 70.00, 90)
    call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 2.00)
    call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
    call RemoveLocation(udg_TempLoc)
    // HealingStop
    call SetUnitLifeBJ(GetTriggerUnit(), ( GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit()) + ( GetUnitStateSwap(UNIT_STATE_MAX_LIFE, GetTriggerUnit()) / 10.00 ) ))
    call AddSpecialEffectTargetUnitBJ("overhead", GetTriggerUnit(), "Abilities\\Spells\\Undead\\VampiricAura\\VampiricAuraTarget.mdl")
    call DestroyEffectBJ(GetLastCreatedEffectBJ())
endfunction

