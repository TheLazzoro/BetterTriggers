    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_TempInt=GetUnitLevel(GetTriggerUnit())
        call AdjustPlayerStateBJ(udg_TempInt, Player(0), PLAYER_STATE_RESOURCE_GOLD)
        call AdjustPlayerStateBJ(udg_TempInt, Player(1), PLAYER_STATE_RESOURCE_GOLD)
        call AdjustPlayerStateBJ(udg_TempInt, Player(2), PLAYER_STATE_RESOURCE_GOLD)
        call AdjustPlayerStateBJ(udg_TempInt, Player(3), PLAYER_STATE_RESOURCE_GOLD)
        call AdjustPlayerStateBJ(udg_TempInt, Player(4), PLAYER_STATE_RESOURCE_GOLD)
        call AdjustPlayerStateBJ(udg_TempInt, Player(5), PLAYER_STATE_RESOURCE_GOLD)
        call CreateTextTagUnitBJ(( "+" + I2S(udg_TempInt) ), GetTriggerUnit(), 0, 10.00, 100.00, 100, 0.00, 0)
        call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
        call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 50.00, 90.00)
        call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 3.00)
        call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
