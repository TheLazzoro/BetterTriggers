function Trig_Shadow_Bolt_Actions takes nothing returns nothing
    // BonusMultipliersAdd
    set udg_TempRealBonus=2.30
    // BonusMultipliersEnd
    set udg_TempReal=( I2R(GetHeroStatBJ(bj_HEROSTAT_INT, GetTriggerUnit(), true)) * udg_TempRealBonus )
    if ( Trig_Shadow_Bolt_Func006C() ) then
        set udg_TempReal=( udg_TempReal * 1.11 )
    else
    endif
    if ( Trig_Shadow_Bolt_Func007C() ) then
        set udg_TempReal=( udg_TempReal * 1.14 )
    else
    endif
    if ( Trig_Shadow_Bolt_Func008C() ) then
        set udg_TempReal=( udg_TempReal * 1.22 )
    else
    endif
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_IndexEnchant
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Shadow_Bolt_Func009Func001C() ) then
            set udg_TempReal=( udg_TempReal * 1.10 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set udg_TempInt=GetRandomInt(1, 100)
    if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
        set udg_TempReal=( udg_TempReal * 2.00 )
    else
        call DoNothing()
    endif
    set udg_TempInt=R2I(udg_TempReal)
    call UnitDamageTargetBJ(GetTriggerUnit(), GetSpellTargetUnit(), I2R(udg_TempInt), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
    if ( Trig_Shadow_Bolt_Func014C() ) then
        // HealingStart
        set udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + R2I(udg_TempReal) )
        call MultiboardSetItemValueBJ(udg_Multiboard, 5, ( GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit())) + 1 ), I2S(udg_HealInt[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]))
        set udg_TempLoc=GetUnitLoc(GetTriggerUnit())
        call CreateTextTagLocBJ(I2S(( R2I(GetUnitStateSwap(UNIT_STATE_MAX_LIFE, GetTriggerUnit())) * R2I(udg_ShadowBoltReal[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) )), udg_TempLoc, 60.00, 10.00, 0.00, 100.00, 0.00, 0)
        call SetTextTagPermanentBJ(GetLastCreatedTextTag(), false)
        call SetTextTagVelocityBJ(GetLastCreatedTextTag(), 70.00, 90)
        call SetTextTagLifespanBJ(GetLastCreatedTextTag(), 2.00)
        call SetTextTagFadepointBJ(GetLastCreatedTextTag(), 1.25)
        call RemoveLocation(udg_TempLoc)
        // HealingStop
        call SetUnitLifeBJ(GetTriggerUnit(), ( GetUnitStateSwap(UNIT_STATE_LIFE, GetTriggerUnit()) + ( GetUnitStateSwap(UNIT_STATE_MAX_LIFE, GetTriggerUnit()) * udg_ShadowBoltReal[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] ) ))
        call AddSpecialEffectTargetUnitBJ("overhead", GetTriggerUnit(), "Abilities\\Spells\\Undead\\VampiricAura\\VampiricAuraTarget.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
    else
    endif
    if ( Trig_Shadow_Bolt_Func015C() ) then
        call BlzStartUnitAbilityCooldown(GetTriggerUnit(), 'A026', 8.00)
    else
    endif
endfunction

