        if ( Trig_Flamestrike_Func006Func001C() ) then
            set udg_TempReal=( udg_TempReal * 1.10 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    if ( Trig_Flamestrike_Func007C() ) then
        set udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] + 10 )
        set udg_TempInt=GetRandomInt(1, 100)
        if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
            set udg_TempReal=( udg_TempReal * 2.00 )
        else
            call DoNothing()
        endif
        set udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]=( udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))] - 10 )
    else
        set udg_TempInt=GetRandomInt(1, 100)
        if ( (udg_TempInt <= udg_SpellCrit[GetConvertedPlayerId(GetOwningPlayer(GetTriggerUnit()))]) ) then // INLINED!!
            set udg_TempReal=( udg_TempReal * 2.00 )
        else
            call DoNothing()
        endif
    endif
    set udg_FlamestrikeInt=R2I(udg_TempReal)
    set udg_FlamstrikeLoc=GetSpellTargetLoc()
    call TriggerSleepAction(1.00)
    set udg_TempGroup=GetUnitsInRangeOfLocAll(400.00, udg_FlamstrikeLoc)
    call GroupRemoveGroup(GetUnitsOfTypeIdAll('h00K'), udg_TempGroup)
