			call BlzFrameSetPoint(CombinedCameraSliderSystem___sliderRotation, FRAMEPOINT_TOPLEFT, CombinedCameraSliderSystem___sliderAbove, FRAMEPOINT_BOTTOMLEFT, 0, - udg_CCSS_SliderGap)
		else
			call BlzFrameSetAbsPoint(CombinedCameraSliderSystem___sliderRotation, CombinedCameraSliderSystem___framepointVerticalMain, udg_CCSS_PositionSlidersX, udg_CCSS_PositionSlidersY)
