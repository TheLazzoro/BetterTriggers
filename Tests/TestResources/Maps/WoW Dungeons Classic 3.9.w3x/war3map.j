function Trig_Spec_Priest_Func002C takes nothing returns boolean
    if ( ( GetUnitAbilityLevelSwapped('A04O', GetTriggerUnit()) == 1 ) ) then
        return true
    endif
    if ( ( GetUnitAbilityLevelSwapped('A04P', GetTriggerUnit()) == 1 ) ) then
        return true
    endif
    if ( ( GetUnitAbilityLevelSwapped('A04Q', GetTriggerUnit()) == 1 ) ) then
        return true
