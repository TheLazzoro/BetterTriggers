        return false
    endif
    return true
endfunction

function Trig_CBar_Cast_Actions takes nothing returns nothing
    set udg_CBar_Loop=1
    loop
        exitwhen udg_CBar_Loop > udg_CBar_Abilities_c
        if ( Trig_CBar_Cast_Func001Func001C() ) then
            set udg_CBar_Rnd=GetRandomInt(1, 100)
            if ( Trig_CBar_Cast_Func001Func001Func002C() ) then
                set udg_CBar_Time[udg_CBar_Rnd]=BlzGetAbilityRealLevelField(BlzGetUnitAbility(GetTriggerUnit(), GetSpellAbilityId()), ABILITY_RLF_DURATION_HERO, ( GetUnitAbilityLevelSwapped(GetSpellAbilityId(), GetTriggerUnit()) - 1 ))
            else
                set udg_CBar_Time[udg_CBar_Rnd]=BlzGetAbilityRealLevelField(BlzGetUnitAbility(GetTriggerUnit(), GetSpellAbilityId()), ABILITY_RLF_CASTING_TIME, ( GetUnitAbilityLevelSwapped(GetSpellAbilityId(), GetTriggerUnit()) - 1 ))
            endif
            if ( Trig_CBar_Cast_Func001Func001Func003C() ) then
                set udg_CBar_Time[udg_CBar_Rnd]=4.00
            else
            endif
            if ( Trig_CBar_Cast_Func001Func001Func004C() ) then
                set udg_CBar_Time[udg_CBar_Rnd]=10.00
            else
            endif
            set udg_CBar_Time[udg_CBar_Rnd]=( 1.00 / ( udg_CBar_Time[udg_CBar_Rnd] + 0.20 ) )
            set udg_CBar_Point_A=OffsetLocation(GetUnitLoc(GetTriggerUnit()), - 15.00, 50.00)
            call CreateNUnitsAtLoc(1, 'n032', GetOwningPlayer(GetTriggerUnit()), udg_CBar_Point_A, 270.00)
            call SetUnitUserData(GetLastCreatedUnit(), udg_CBar_Rnd)
            call SetUnitUserData(GetSpellAbilityUnit(), udg_CBar_Rnd)
            call RemoveLocation(udg_CBar_Point_A)
            set udg_CBar_Bar_c[udg_CBar_Rnd]=GetLastCreatedUnit()
        else
        endif
        set udg_CBar_Loop=udg_CBar_Loop + 1
    endloop
endfunction

