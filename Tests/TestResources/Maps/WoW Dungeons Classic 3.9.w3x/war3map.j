function Trig_Ignite_Mana_Conditions takes nothing returns boolean
    if ( not ( IsUnitAliveBJ(gg_unit_n037_0711) == true ) ) then
        return false
    endif
    if ( not ( GetUnitStateSwap(UNIT_STATE_LIFE, gg_unit_n037_0711) < ( I2R(GetUnitLevel(gg_unit_n037_0711)) * ( I2R(udg_Mode) * 190.00 ) ) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Ignite_Mana_Func007C takes nothing returns boolean
    if ( not ( CountUnitsInGroup(udg_TempBossGroup) == 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Ignite_Mana_Actions takes nothing returns nothing
    set udg_TempBossGroup=GetUnitsInRangeOfLocAll(900.00, GetUnitLoc(gg_unit_n037_0711))
    call GroupRemoveGroup(GetUnitsOfPlayerAll(Player(PLAYER_NEUTRAL_AGGRESSIVE)), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsOfTypeIdAll('h00K'), udg_TempBossGroup)
    call GroupRemoveGroup(GetUnitsInRangeOfLocAll(100.00, GetUnitLoc(gg_unit_n037_0711)), udg_TempBossGroup)
    if ( Trig_Ignite_Mana_Func007C() ) then
        call DestroyGroup(udg_TempBossGroup)
        return
    else
        set udg_TempUnit=GroupPickRandomUnit(udg_TempBossGroup)
        set udg_TempBossReal=( GetUnitStateSwap(UNIT_STATE_MAX_MANA, udg_TempUnit) / 2.00 )
        call SetUnitManaBJ(udg_TempUnit, ( GetUnitStateSwap(UNIT_STATE_MANA, udg_TempUnit) - udg_TempBossReal ))
        call UnitDamageTargetBJ(gg_unit_n037_0711, udg_TempUnit, ( udg_TempBossReal * udg_ModeDmgReal ), ATTACK_TYPE_NORMAL, DAMAGE_TYPE_NORMAL)
        call AddSpecialEffectTargetUnitBJ("origin", udg_TempUnit, "war3mapImported\\Conflagrate.mdx")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        call DestroyGroup(udg_TempBossGroup)
    endif
endfunction

