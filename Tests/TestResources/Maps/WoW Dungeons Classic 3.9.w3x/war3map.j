 function CombinedCameraSliderSystem___LoadToc takes string s returns nothing
		if not BlzLoadTOCFile(s) then
			call BJDebugMsg("|cffff0000Failed to Load: " + s + "|r \nYou need to import the |cffffcc00templates.toc|r file.")
		endif
	endfunction

 function CombinedCameraSliderSystem___UpdateCam takes nothing returns nothing
  local integer i= GetPlayerId(GetEnumPlayer())
  local unit u= udg_CCSS_TargetUnit[i + 1]
  local real facing= GetUnitFacing(u)
  local real moveSpeed= GetUnitMoveSpeed(u)
  local real x= GetUnitX(u)
  local real y= GetUnitY(u)
  local real hDir= 0
        local real vDir= 0
		call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_TARGET_DISTANCE, BlzFrameGetValue(CombinedCameraSliderSystem___sliderDistance), udg_CCSS_CamUpdateInterval)
		
		if ( udg_CCSS_3rdPersonCamMode ) then // Check if Third Person Camera mode is enabled
			if ( ( ( udg_CCSS_UseWASDKeys ) or ( udg_CCSS_UseNumpadKeys ) ) and ( u != null ) ) then // Unit WASD control. Obviously, run these only if the WASD keys are enabled and there is a Target Unit
				if ( CombinedCameraSliderSystem___wKeyIsDown[i] ) then
					set CombinedCameraSliderSystem___forward[i]=1
				else
					set CombinedCameraSliderSystem___forward[i]=0
				endif

				if ( CombinedCameraSliderSystem___aKeyIsDown[i] ) then
					set CombinedCameraSliderSystem___sideways[i]=CombinedCameraSliderSystem___sideways[i] - 1
				endif

				if ( CombinedCameraSliderSystem___dKeyIsDown[i] ) then
					set CombinedCameraSliderSystem___sideways[i]=CombinedCameraSliderSystem___sideways[i] + 1
				endif

				set CombinedCameraSliderSystem___xTarget[i]=x
				set CombinedCameraSliderSystem___yTarget[i]=y

				if ( ( moveSpeed / udg_CCSS_WASDMoveSpeedModifier ) < udg_CCSS_WASDMoveMinDistance ) then
					set moveSpeed=udg_CCSS_WASDMoveMinDistance
				else
					set moveSpeed=moveSpeed / udg_CCSS_WASDMoveSpeedModifier
				endif

				if ( CombinedCameraSliderSystem___forward[i] == 1 ) then
					set CombinedCameraSliderSystem___xTarget[i]=CombinedCameraSliderSystem___xTarget[i] + Cos(Deg2Rad(facing)) * ( moveSpeed )
					set CombinedCameraSliderSystem___yTarget[i]=CombinedCameraSliderSystem___yTarget[i] + Sin(Deg2Rad(facing)) * ( moveSpeed )

					if ( CombinedCameraSliderSystem___sideways[i] < 0 ) then
						set CombinedCameraSliderSystem___xTarget[i]=CombinedCameraSliderSystem___xTarget[i] + Cos(Deg2Rad(facing + 90)) * udg_CCSS_WASDTurnSpeed
						set CombinedCameraSliderSystem___yTarget[i]=CombinedCameraSliderSystem___yTarget[i] + Sin(Deg2Rad(facing + 90)) * udg_CCSS_WASDTurnSpeed
						call IssuePointOrder(u, "move", CombinedCameraSliderSystem___xTarget[i], CombinedCameraSliderSystem___yTarget[i])
						set CombinedCameraSliderSystem___sideways[i]=0
					endif
				
					if ( CombinedCameraSliderSystem___sideways[i] > 0 ) then
						set CombinedCameraSliderSystem___xTarget[i]=CombinedCameraSliderSystem___xTarget[i] + Cos(Deg2Rad(facing + 270)) * udg_CCSS_WASDTurnSpeed
						set CombinedCameraSliderSystem___yTarget[i]=CombinedCameraSliderSystem___yTarget[i] + Sin(Deg2Rad(facing + 270)) * udg_CCSS_WASDTurnSpeed
						call IssuePointOrder(u, "move", CombinedCameraSliderSystem___xTarget[i], CombinedCameraSliderSystem___yTarget[i])
						set CombinedCameraSliderSystem___sideways[i]=0
					endif
				else
					if ( CombinedCameraSliderSystem___sideways[i] == - 1 ) then
						call SetUnitFacing(u, facing + ( udg_CCSS_WASDStationaryTurnSpeed ))
						set CombinedCameraSliderSystem___sideways[i]=0
					endif

					if ( CombinedCameraSliderSystem___sideways[i] == 1 ) then
						call SetUnitFacing(u, facing - ( udg_CCSS_WASDStationaryTurnSpeed ))
						set CombinedCameraSliderSystem___sideways[i]=0
					endif

					if ( udg_CCSS_SButtonOrder != "" ) then
						if ( CombinedCameraSliderSystem___sKeyIsDown[i] ) then
							call IssueImmediateOrder(u, udg_CCSS_SButtonOrder)
						endif
					endif

					set CombinedCameraSliderSystem___fullTurnDelay[i]=CombinedCameraSliderSystem___fullTurnDelay[i] - udg_CCSS_CamUpdateInterval
					if ( CombinedCameraSliderSystem___fullTurnDelay[i] <= 0 ) then
						if ( udg_CCSS_SButton180Turn ) then
							if ( CombinedCameraSliderSystem___sKeyIsDown[i] ) then
								set CombinedCameraSliderSystem___fullTurnDelay[i]=udg_CCSS_CamUpdateInterval * 10
								call SetUnitFacing(u, facing - 180)
							endif
						endif

						if ( udg_CCSS_XButton180Turn ) then
							if ( CombinedCameraSliderSystem___xKeyIsDown[i] ) then
								set CombinedCameraSliderSystem___fullTurnDelay[i]=udg_CCSS_CamUpdateInterval * 10
								call SetUnitFacing(u, facing - 180)
							endif
						endif
					endif
				endif

				set CombinedCameraSliderSystem___orderDelay[i]=CombinedCameraSliderSystem___orderDelay[i] - udg_CCSS_WASDMoveOrderInterval
				if ( CombinedCameraSliderSystem___orderDelay[i] <= 0 ) then
					set CombinedCameraSliderSystem___orderDelay[i]=1
					if ( ( CombinedCameraSliderSystem___forward[i] != 0 ) or ( CombinedCameraSliderSystem___sideways[i] != 0 ) ) then
						call IssuePointOrder(u, "move", CombinedCameraSliderSystem___xTarget[i], CombinedCameraSliderSystem___yTarget[i])
						set CombinedCameraSliderSystem___sideways[i]=0
					endif
				endif
			endif

            if ( udg_CCSS_UseArrowKeys ) then // Only run these if the Arrow Keys are used
                
                // Horizontal Key Register
                if ( CombinedCameraSliderSystem___leftArrowIsDown[i] ) then
                    if ( udg_CCSS_InvertHorizontalMovement ) then
                        if ( CombinedCameraSliderSystem___horizontalChange[i] + ( udg_CCSS_HorizontalSpeed * 3 ) > - udg_CCSS_HorizontalArrowMoveLimit ) then
                            set hDir=hDir - 1
                        endif
                    else
                        if ( CombinedCameraSliderSystem___horizontalChange[i] - ( udg_CCSS_HorizontalSpeed * 3 ) < udg_CCSS_HorizontalArrowMoveLimit ) then
                            set hDir=hDir + 1
                        endif
                    endif
                endif
                if ( CombinedCameraSliderSystem___rightArrowIsDown[i] ) then
                    if ( udg_CCSS_InvertHorizontalMovement ) then
                        if ( CombinedCameraSliderSystem___horizontalChange[i] - ( udg_CCSS_HorizontalSpeed * 3 ) < udg_CCSS_HorizontalArrowMoveLimit ) then
                            set hDir=hDir + 1
                        endif
                    else
                        if ( CombinedCameraSliderSystem___horizontalChange[i] + ( udg_CCSS_HorizontalSpeed * 3 ) > - udg_CCSS_HorizontalArrowMoveLimit ) then
                            set hDir=hDir - 1
                        endif
                    endif
                endif
                
                set CombinedCameraSliderSystem___horizontalChange[i]=CombinedCameraSliderSystem___horizontalChange[i] + ( hDir * udg_CCSS_HorizontalSpeed )
                if ( ( hDir == 0 ) and ( CombinedCameraSliderSystem___horizontalChange[i] != 0 ) ) then
                    if ( CombinedCameraSliderSystem___horizontalChange[i] > 0 ) then
                        set CombinedCameraSliderSystem___horizontalChange[i]=CombinedCameraSliderSystem___horizontalChange[i] - ( udg_CCSS_HorizontalSpeed * udg_CCSS_HorizontalReturnSpeed )
                        if ( CombinedCameraSliderSystem___horizontalChange[i] < udg_CCSS_HorizontalSpeed ) then
                            set CombinedCameraSliderSystem___horizontalChange[i]=0
                        endif
                    else
                        set CombinedCameraSliderSystem___horizontalChange[i]=CombinedCameraSliderSystem___horizontalChange[i] + ( udg_CCSS_HorizontalSpeed * udg_CCSS_HorizontalReturnSpeed )
                        if ( CombinedCameraSliderSystem___horizontalChange[i] > udg_CCSS_HorizontalSpeed ) then
                            set CombinedCameraSliderSystem___horizontalChange[i]=0
                        endif
                    endif
                endif

                // Vertical Key Register
                if ( CombinedCameraSliderSystem___upArrowIsDown[i] ) then
                    if ( udg_CCSS_InvertVerticalMovement ) then
                        if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] + ( udg_CCSS_HorizontalSpeed * 2 ) ) > ( udg_CCSS_AngleOfAttackMin ) ) then
                            set vDir=vDir - 1
                        endif
                    else
                        if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] - ( udg_CCSS_HorizontalSpeed * 2 ) ) < ( udg_CCSS_AngleOfAttackMax ) ) then // Don't allow the number to grow if camera had stopped at the limit
                            set vDir=vDir + 1
                        endif
                    endif
                endif
                if ( CombinedCameraSliderSystem___downArrowIsDown[i] ) then
                    if ( udg_CCSS_InvertVerticalMovement ) then
                        if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] - ( udg_CCSS_HorizontalSpeed * 2 ) ) < ( udg_CCSS_AngleOfAttackMax ) ) then
                            set vDir=vDir + 1
                        endif
                    else
                        if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] + ( udg_CCSS_HorizontalSpeed * 2 ) ) > ( udg_CCSS_AngleOfAttackMin ) ) then
                            set vDir=vDir - 1
                        endif
                    endif
                endif

                set CombinedCameraSliderSystem___verticalChange[i]=CombinedCameraSliderSystem___verticalChange[i] + ( vDir * udg_CCSS_VerticalSpeed )
                if ( ( vDir == 0 ) and ( CombinedCameraSliderSystem___verticalChange[i] != 0 ) ) then
                    if ( CombinedCameraSliderSystem___verticalChange[i] > 0 ) then
                        set CombinedCameraSliderSystem___verticalChange[i]=CombinedCameraSliderSystem___verticalChange[i] - ( udg_CCSS_VerticalSpeed * udg_CCSS_VerticalReturnSpeed )
                        if ( CombinedCameraSliderSystem___verticalChange[i] < udg_CCSS_VerticalSpeed ) then
                            set CombinedCameraSliderSystem___verticalChange[i]=0
                        endif
                    else
                        set CombinedCameraSliderSystem___verticalChange[i]=CombinedCameraSliderSystem___verticalChange[i] + ( udg_CCSS_VerticalSpeed * udg_CCSS_VerticalReturnSpeed )
                        if ( CombinedCameraSliderSystem___verticalChange[i] > udg_CCSS_VerticalSpeed ) then
                            set CombinedCameraSliderSystem___verticalChange[i]=0
                        endif
                    endif
                endif

                
            endif
            
            // Apply arrow key changes
            if ( u != null ) then // Check if our player has a Target Unit
                // Horizontal
                if ( CombinedCameraSliderSystem___horizontalChange[i] > udg_CCSS_HorizontalArrowMoveLimit ) then
                    call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ROTATION, facing + udg_CCSS_HorizontalArrowMoveLimit, udg_CCSS_CamUpdateInterval) // Prevent camera from rolling around the unit forever
                else
                    if ( CombinedCameraSliderSystem___horizontalChange[i] < - udg_CCSS_HorizontalArrowMoveLimit ) then
                        call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ROTATION, facing - udg_CCSS_HorizontalArrowMoveLimit, udg_CCSS_CamUpdateInterval)
                    else
                        call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ROTATION, facing + CombinedCameraSliderSystem___horizontalChange[i], udg_CCSS_CamUpdateInterval)
                    endif
                endif
                
                if ( GetLocalPlayer() == Player(i) ) then // Lock the rotation bar. Purely cosmetic, can be removed if, for example, causes desync problems. (The bar won't affect rotation even if not locked)
                     call BlzFrameSetEnable(CombinedCameraSliderSystem___sliderRotation, false)
                endif

                // Vertical
                if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] ) > ( udg_CCSS_AngleOfAttackMax ) ) then // Prevent camera from overstepping angle of attack slider maximum and minimum.
                    call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ANGLE_OF_ATTACK, udg_CCSS_AngleOfAttackMax, udg_CCSS_CamUpdateInterval)
                else
                    if ( ( BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i] ) < ( udg_CCSS_AngleOfAttackMin ) ) then
                        call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ANGLE_OF_ATTACK, udg_CCSS_AngleOfAttackMin, udg_CCSS_CamUpdateInterval)
                    else
                        call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ANGLE_OF_ATTACK, BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack) + CombinedCameraSliderSystem___verticalChange[i], udg_CCSS_CamUpdateInterval)
                    endif
                endif

                // Add Unit Height to Camera
                if ( udg_CCSS_AddUnitHeightToCamera ) then
		            call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ZOFFSET, BlzFrameGetValue(CombinedCameraSliderSystem___sliderHeight) + GetUnitFlyHeight(u), udg_CCSS_CamUpdateInterval)
                endif

            else // If doesn't have Target Unit
                // Horizontal
                call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ROTATION, BlzFrameGetValue(CombinedCameraSliderSystem___sliderRotation) + 90, udg_CCSS_CamUpdateInterval) // +90 Because I want the default rotation angle (90) to be zero for simplicity's sake
                
                if ( ( GetLocalPlayer() == GetEnumPlayer() ) and ( udg_CCSS_RotationEnabled ) ) then
                    call BlzFrameSetEnable(CombinedCameraSliderSystem___sliderRotation, true)
                endif

                // Vertical
                call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ANGLE_OF_ATTACK, BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack), udg_CCSS_CamUpdateInterval)

                // Height
                call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ZOFFSET, BlzFrameGetValue(CombinedCameraSliderSystem___sliderHeight), udg_CCSS_CamUpdateInterval)
            endif
        else // Only run this if 3rdPC mode is not used
            call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ROTATION, BlzFrameGetValue(CombinedCameraSliderSystem___sliderRotation) + 90, udg_CCSS_CamUpdateInterval) // +90 so that 0 on the slider is the default WC3 angle
            call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ANGLE_OF_ATTACK, BlzFrameGetValue(CombinedCameraSliderSystem___sliderAngleOfAttack), udg_CCSS_CamUpdateInterval)
		    call SetCameraFieldForPlayer(Player(i), CAMERA_FIELD_ZOFFSET, BlzFrameGetValue(CombinedCameraSliderSystem___sliderHeight), udg_CCSS_CamUpdateInterval)
        endif

        set u=null
	endfunction

    function CombinedCameraSliderSystem___HumanPlayersPlaying takes nothing returns boolean
        if ( GetPlayerController(GetFilterPlayer()) == MAP_CONTROL_USER ) and ( GetPlayerSlotState(GetFilterPlayer()) == PLAYER_SLOT_STATE_PLAYING ) then
            return true
        else
            return false
        endif
    endfunction

