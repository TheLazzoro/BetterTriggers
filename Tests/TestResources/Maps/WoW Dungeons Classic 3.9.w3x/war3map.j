    if ( not ( IsUnitAliveBJ(gg_unit_n01E_0149) == true ) ) then
        return false
    endif
    if ( not ( GetUnitStateSwap(UNIT_STATE_LIFE, gg_unit_n01E_0149) < ( I2R(GetUnitLevel(gg_unit_n01E_0149)) * ( I2R(udg_Mode) * 180.00 ) ) ) ) then
