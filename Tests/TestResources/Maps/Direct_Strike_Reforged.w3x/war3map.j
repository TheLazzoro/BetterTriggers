	if ( CountUnitsInGroup(udg_HookCasterGroup) == 0 ) then
	call DisableTrigger(GetTriggeringTrigger())
	else
	endif

	call ForGroup(udg_HookCasterGroup, function Trig_Hook_Periodic_277)

endfunction


