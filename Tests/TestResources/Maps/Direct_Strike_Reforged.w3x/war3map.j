	set s__udg_PlayerQueue[GetForLoopIndexA()]= 1
	else
	endif

	endif

	endif

	else
	endif

	set bj_forLoopAIndex=bj_forLoopAIndex + 1
endloop
	set bj_forLoopBIndex=bj_forLoopBIndex + 1
endloop
	call DestroyGroup(s__udg_UnitGroupBuildings[1])
