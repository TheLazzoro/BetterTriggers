        // update visual
        if ToggleIconButton_Mode[object] == ToggleIconButton_MODE_SHARED or GetLocalPlayer() == p then
            if not enable then
                call BlzFrameSetTexture(ToggleIconButton_Icon[object], ToggleIconButton_TextureOff[object], 0, false)
