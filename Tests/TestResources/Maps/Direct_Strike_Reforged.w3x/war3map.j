                call BlzFrameSetTexture(ToggleIconButton_IconPushed[object], ToggleIconButton_Texture[object], 0, false)
                call BlzFrameSetText(ToggleIconButton_ToolTip[object], ToggleIconButton_Text[object])
            endif
        endif
    endfunction
    

