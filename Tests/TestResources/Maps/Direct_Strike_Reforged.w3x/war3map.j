                    set frameXPOS=0
                    set incr=0
                elseif ( r == 6 and i == 10 ) then
                    set frameYPOS=frameYPOS + 0.09
                    set frameXPOS=0
                    set incr=0
                elseif ( r == 7 and i == 10 ) then
                    set frameYPOS=frameYPOS + 0.09
                    set frameXPOS=0
                    set incr=0
                elseif ( r == 8 and i == 9 ) then
                    set frameYPOS=frameYPOS + 0.09
                    set frameXPOS=0
                    set incr=0
                endif
            endif
            set incr=incr + 1

            set i=i + 1
